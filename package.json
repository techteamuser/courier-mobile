{
  "name": "CourierMobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "androidClean": "cd android && ./gradlew clean && cd ../",
    "android:dev": "react-native run-android --variant=developmentdebug",
    "android:dev-release": "react-native run-android --variant=developmentrelease",
    "android:staging": "react-native run-android --variant=stagingdebug",
    "android:staging-release": "react-native run-android --variant=stagingrelease",
    "android:prod": "react-native run-android --variant=productiondebug",
    "android:prod-release": "react-native run-android --variant=productionrelease",
    "ios": "react-native run-ios",
    "ios:dev": "react-native run-ios --scheme 'CourierMobileDev'",
    "ios:dev-release": "react-native run-ios --configuration Release --scheme 'CourierMobileDev'",
    "ios:staging": "react-native run-ios --scheme 'CourierMobileStg'",
    "ios:staging-release": "react-native run-ios --configuration Release --scheme 'CourierMobileStg'",
    "ios:prod": "react-native run-ios --scheme 'CourierMobile'",
    "ios:prod-release": "react-native run-ios --configuration Release --scheme 'CourierMobile'",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "build:tailwind": "tailwindcss --input input.css --output tailwind.css --no-autoprefixer && tailwind-rn",
    "dev:tailwind": "concurrently \"tailwindcss --input input.css --output tailwind.css --no-autoprefixer --watch\" \"tailwind-rn --watch\""
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-native-community/netinfo": "^9.3.7",
    "@react-navigation/bottom-tabs": "^6.5.1",
    "@react-navigation/native": "^6.1.1",
    "@react-navigation/stack": "^6.3.9",
    "numeral": "^2.0.6",
    "prop-types": "^15.8.1",
    "react": "18.1.0",
    "react-hook-form": "^7.40.0",
    "react-native": "0.70.6",
    "react-native-config": "^1.4.11",
    "react-native-device-info": "^10.3.0",
    "react-native-error-boundary": "^1.1.16",
    "react-native-flash-message": "^0.3.1",
    "react-native-gesture-handler": "^2.8.0",
    "react-native-offline": "^6.0.0",
    "react-native-qrcode-svg": "^6.1.2",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^3.18.2",
    "react-native-share": "^8.0.1",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^13.6.0",
    "react-query": "^3.39.2",
    "tailwind-rn": "^4.2.0",
    "use-debounce": "^9.0.2",
    "zustand": "^4.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "concurrently": "^7.6.0",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "0.72.3",
    "postcss": "^8.4.20",
    "react-native-svg-transformer": "^1.0.0",
    "react-test-renderer": "18.1.0",
    "tailwindcss": "^3.2.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
